  journal = {Structure (London, England : 1993)},
  doi = {10.1016/j.str.2014.08.013},
  author = {Rey, Martial and Sarpe, Vladimir and Burns, Kyle and Buse, Joshua and Baker, Charles A.H. and {van Dijk}, Marc and Wordeman, Linda and Bonvin, Alexandre M.J.J. and Schriemer, David C.},
