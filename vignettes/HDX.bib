  year = {2011},
  pages = {D663-D669},
  file = {/home/michal/Zotero/storage/W5CN553L/Huang i in. - 2011 - ASD a comprehensive database of allosteric protei.pdf},
  pmid = {21051350},
  pmcid = {PMC3013650}
}

